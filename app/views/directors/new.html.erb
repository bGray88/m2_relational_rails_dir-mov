<%= form_with url:"/directors", method: :post, local: true do |f| %>
  <%= f.label :text, "Enter a new director:" %><br>
  <%= f.label :name, "Name:" %><%= f.text_field :name %><br>
  <%= f.label :hometown, "Hometown:" %><%= f.text_field :hometown %><br>
  <%= f.label :alive, "Living:" %>
  <%= f.radio_button :alive, "true" %><%= f.label :alive_true, "true" %>
  <%= f.radio_button :alive, "false" %><%= f.label :alive_false, "false" %><br>
  <%= f.label :age, "Age:" %><%= f.number_field :age %><br>
  <%= f.submit 'Create Director' %>
<% end %>
