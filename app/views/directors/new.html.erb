<form action="/directors" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <p>Enter a new director:</p>
  <input type='text' name='director[name]'/><br/>
  <input type='text' name='director[hometown]'/><br/>
  <input type='text' name='director[alive]'/><br/>
  <input type='text' name='director[age]'/><br/>
  <input type='submit'/>
</form>
